{
  "name": "users-management-api",
  "version": "1.0.0",
  "description": "Api for users management",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx src/server.ts | pino-pretty",
    "build": "tsup src",
    "test": "vitest",
    "db:migrate": "pnpm dlx prisma migrate dev",
    "db:deploy": "pnpm dlx prisma migrate deploy",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint:check": "pnpm dlx @biomejs/biome format  ./src",
    "lint:apply": "pnpm dlx @biomejs/biome lint --write ./src",
    "lint:fix": "pnpm dlx @biomejs/biome check --write ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jwt": "^0.2.0",
    "pg": "^8.12.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.2.2",
    "tsup": "^8.2.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "20.16.5",
    "prisma": "5.19.1",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}