{
  "name": "users-management-api",
  "version": "1.0.0",
  "description": "Api for users management",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx src/server.ts | pino-pretty",
    "build": "tsup src",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "pnpm dlx prisma migrate dev",
    "db:deploy": "pnpm dlx prisma migrate deploy",
    "db:reset": "pnpm dlx prisma migrate reset",
    "test:coverage": "jest run --coverage",
    "lint:check": "pnpm dlx @biomejs/biome format  ./src",
    "lint:apply": "pnpm dlx @biomejs/biome lint --write ./src",
    "lint:fix": "pnpm dlx @biomejs/biome check --write ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "ts-jest": "^29.3.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.14.0",
    "prisma": "6.6.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
  
}